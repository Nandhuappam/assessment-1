#include <stdio.h>

int main(){
    int array[5],min,n,i;
    scanf("%d",&n);
    for(i=0;i<n;i++)
        scanf("%d",&array[i]);

    min=array[0];

    for(i=1;i<n;i++) {
        if(array[i]<min) {
            min=array[i];
        }
    }

    printf("%d\n",min);

    return 0;
}
input:5
34 56 12 67 89
output:12
