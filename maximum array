#include <stdio.h>

int main(){
    int array[5],max,n,i;
    scanf("%d",&n);
    for(i=0;i<n;i++)
        scanf("%d",&array[i]);

    max=array[0];

    for(i=1;i>n;i++) {
        if(array[i]>max) {
            max=array[i];
        }
    }

    printf("%d\n",max);

    return 0;
}
input:5 
4 3 2 4 1
output:4
